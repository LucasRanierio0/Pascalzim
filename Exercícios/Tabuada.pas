Program Tabuada ;
Var N : integer;
Begin
	write ('Digite um número: ');
	readln (N);
	writeln (N, ' x ', 0, ' = ', N*0:0:0);
	writeln (N, ' x ', 1, ' = ', N*1:0:0);
	writeln (N, ' x ', 2, ' = ', N*2:0:0);
	writeln (N, ' x ', 3, ' = ', N*3:0:0);
	writeln (N, ' x ', 4, ' = ', N*4:0:0);
	writeln (N, ' x ', 5, ' = ', N*5:0:0);
	writeln (N, ' x ', 6, ' = ', N*6:0:0);
	writeln (N, ' x ', 7, ' = ', N*7:0:0);
	writeln (N, ' x ', 8, ' = ', N*8:0:0);
	writeln (N, ' x ', 9, ' = ', N*9:0:0);
	writeln (N, ' x ', 10, ' = ', N*10:0:0);
	readln;  
End.